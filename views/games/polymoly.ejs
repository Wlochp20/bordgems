                            <%= isNaN(locals.lobby.gameOptions.board[i].additional.rent[locals.lobby.gameOptions.board[i].additional.amountLodges] * 1) ? "" : locals.lobby.gameOptions.board[i].additional.rent[locals.lobby.gameOptions.board[i].additional.amountLodges] * locals.lobby.gameOptions.board[i].additional.multiplier + "$" %>
                        </div>
                    <% } %>
                </div>
            <% } %>
        </div>
        <div class="right">
            <% for( let i = 24; i < 32; i++ ) { %>
                <div type="<%= locals.lobby.gameOptions.board[i].type %>" index="<%= i %>" class="<% if (locals.lobby.gameOptions.board[i].additional.isFestival) { %>festival<% } %> <% if (locals.lobby.gameOptions.board[i].additional.isChampions) { %>champions<% } %> <% if (locals.lobby.gameOptions.board[i].additional.electricityOff != 0 && locals.lobby.gameOptions.board[i].additional.electricityOff != undefined) { %>electricityOff<% } %>">
                    <% if (locals.lobby.gameOptions.board[i].type == 1) { %>
                        <div class="color" style="--field-color: <%= locals.lobby.gameOptions.board[i].additional.color %>;"></div>
                    <% } %>
                    <p><%= locals.lobby.gameOptions.board[i].name %></p>
                    <div class="players"></div>
                    <% if (locals.lobby.gameOptions.board[i].type < 3) { %>
                        <div class="price" owner="<%= locals.lobby.gameOptions.board[i].additional.owner %>">
                            <%= isNaN(locals.lobby.gameOptions.board[i].additional.rent[locals.lobby.gameOptions.board[i].additional.amountLodges] * 1) ? "" : locals.lobby.gameOptions.board[i].additional.rent[locals.lobby.gameOptions.board[i].additional.amountLodges] * locals.lobby.gameOptions.board[i].additional.multiplier + "$" %>
                        </div>
                    <% } %>
                </div>
            <% } %>
            <div type="<%= locals.lobby.gameOptions.board[0].type %>" index="<%= 0 %>">
                <p><%= locals.lobby.gameOptions.board[0].name %></p>
                <div class="players"></div>
            </div>
        </div>
        <div class="center">
            <div class="money"></div>
            <button class="move">Ruch</button>
            <div class="trade">
                <select>
                    <% for( let i = 0; i < locals.lobby.players.length; i++ ) { %>
                        <% if (i != locals.playerIndex) { %>
                            <option value="<%= i %>"><%= locals.lobby.players[i].name %></option>
                        <% } %>
                    <% } %>
                </select>
                <button class="show">Pokaż towary</button>

                <div class="trades">
                    <div class="tradeFrom">

                    </div>
                    <div class="tradeFor">

                    </div>
                </div>
            </div>
        </div>
    </div>

<%- contentFor('script') %> 
    <script> 
        let playerIndex = JSON.parse('<%= locals.playerIndex %>');
        let playerTurn = JSON.parse('<%= locals.lobby.queue %>');
        let playersAmount = JSON.parse('<%= locals.lobby.players.length %>');
        let playersPosition = JSON.parse('[<%= locals.lobby.gameOptions.positions %>]');
        let playersField = JSON.parse('[<%= locals.lobby.gameOptions.onField %>]');
        let playersMoney = JSON.parse('[<%= locals.lobby.gameOptions.money %>]');
    </script>
    <script src="/js/games/polymoly.js"></script>